{
    "name": "vsburger",
    "displayName": "VSBurger",
    "description": "Order Burgers from within VS Code.",
    "icon": "images/icon.png",
    "version": "1.0.0",
    "publisher": "narimane",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "license": "MIT",
    "keywords": [
        "order burger vsburger"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/narimantos/vsburger.git"
    },
    "homepage": "https://github.com/narimantos/vsburger",
    "bugs": {
        "url": "https://github.com/narimantos/vsburger/issues"
    },
    "activationEvents": [
        "onCommand:extension.orderBurger"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.orderBurger",
                "title": "Order Burger"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "VSBurger Configuration",
            "properties": {
                "vsburger.customBurger": {
                    "type": "string",
                    "description": "Specify the URL to your favourite burger.",
                    "default": "https://mcdelivery.mcdonalds.nl/"
                }
            }
        },
        "keybindings": [
            {
                "command": "extension.orderBurger",
                "key": "ctrl+f2",
                "mac": "cmd+f2"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "extension.orderBurger",
                    "group": "navigation@1"
                }
            ],
            "explorer/context": [
                {
                    "command": "extension.orderBurger",
                    "group": "navigation@1"
                }
            ]
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^18.16.3",
        "@types/mocha": "^2.2.42"
    }
}
